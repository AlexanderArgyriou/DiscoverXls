<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <ThresholdFilter level="INFO"/>
    <Properties>
        <property name="filePattern">%d{yyyy-MM-dd}</property>
    </Properties>
    <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%d %-5p: %C{2} - %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="ALL">
            <AppenderRef ref="STDOUT"/>
        </Root>
    </Loggers>
</Configuration>